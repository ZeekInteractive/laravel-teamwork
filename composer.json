{
  "name": "zeek/laravel-teamwork",
  "description": "A Laravel wrapper for the Teamwork API",
  "type": "library",
  "license": "CC-BY-NC-4.0",
  "autoload": {
    "psr-4": {
      "SquareBit\\Dovetail\\": "src/",
      "SquareBit\\Dovetail\\Commands\\": "commands/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SquareBit\\Dovetail\\Test\\": "tests/"
    }
  },
  "require": {
    "illuminate/support": "5.3.*||5.4.*||5.5.*|6.*|7.*",
    "guzzlehttp/guzzle": "~6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SquareBit\\Dovetail\\ServiceProvider"
      ],
      "aliases": {
        "Dovetail": "SquareBit\\Dovetail\\Facade"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  }
}
